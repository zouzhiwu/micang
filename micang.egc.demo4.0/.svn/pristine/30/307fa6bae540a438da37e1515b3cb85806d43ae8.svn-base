package com.game.entity;

/**
 * 章节详情表
 * 
 * @author gf @2019/11/14
 *
 */
public class ChapterInfo {

	/** 章节详情唯一标识，自增 **/
	private Long id;

	/** 用户id **/
	private Long accountId;

	/** 章节模板id **/
	private Integer chapterId;

	/** 难度等级 **/
	private Integer difficulty;
	/**
	 * 章节内 关卡和得星信息 PointsBean json LIST类型
	 */
	private String points;

	/** 关卡宝箱1 **/
	private Integer starLeveBox1;

	/** 关卡宝箱2 **/
	private Integer starLeveBox2;

	/** 关卡宝箱3 **/
	private Integer starLeveBox3;

	/** 章节宝箱 **/
	private Integer chapterBox;
	private Integer over;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getAccountId() {
		return accountId;
	}

	public void setAccountId(Long accountId) {
		this.accountId = accountId;
	}

	public Integer getChapterId() {
		return chapterId;
	}

	public void setChapterId(Integer chapterId) {
		this.chapterId = chapterId;
	}

	public Integer getDifficulty() {
		return difficulty;
	}

	public void setDifficulty(Integer difficulty) {
		this.difficulty = difficulty;
	}

	public String getPoints() {
		return points;
	}

	public void setPoints(String points) {
		this.points = points;
	}

	public Integer getStarLeveBox1() {
		return starLeveBox1;
	}

	public void setStarLeveBox1(Integer starLeveBox1) {
		this.starLeveBox1 = starLeveBox1;
	}

	public Integer getStarLeveBox2() {
		return starLeveBox2;
	}

	public void setStarLeveBox2(Integer starLeveBox2) {
		this.starLeveBox2 = starLeveBox2;
	}

	public Integer getStarLeveBox3() {
		return starLeveBox3;
	}

	public void setStarLeveBox3(Integer starLeveBox3) {
		this.starLeveBox3 = starLeveBox3;
	}

	public Integer getChapterBox() {
		return chapterBox;
	}

	public void setChapterBox(Integer chapterBox) {
		this.chapterBox = chapterBox;
	}

	public Integer getOver() {
		return over;
	}

	public void setOver(Integer over) {
		this.over = over;
	}
}
