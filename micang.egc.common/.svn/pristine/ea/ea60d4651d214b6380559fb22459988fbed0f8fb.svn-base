package com.common.template;

public class ArenaDailyRankingRewardsTemplate  extends TemplateBase{
	private int UpperRankingLimit;	
	private int LowerRankingLimit;	
	private short HonorPoint; 
	private short	Diamonds; 
	private int	Golds;
	
	public ArenaDailyRankingRewardsTemplate(int id,int upperRankingLimit, int lowerRankingLimit, short honorPoint,
			short diamonds, int golds) {
		super.setId(id);;
		UpperRankingLimit = upperRankingLimit;
		LowerRankingLimit = lowerRankingLimit;
		HonorPoint = honorPoint;
		Diamonds = diamonds;
		Golds = golds;
	}
	public int getUpperRankingLimit() {
		return UpperRankingLimit;
	}
	public int getLowerRankingLimit() {
		return LowerRankingLimit;
	}
	public short getHonorPoint() {
		return HonorPoint;
	}
	public short getDiamonds() {
		return Diamonds;
	}
	public int getGolds() {
		return Golds;
	} 
	
	
}
