package com.common.template;

public class ArenaTopRankingRewardsTemplate  extends TemplateBase{
	private String Desc;	
	private int TopRanking;	
	private short HonorPoint; 
	private short	Diamonds; 
	private int	Golds; 
	private String	ItemRewards; 
	private String	ItemNum;
	
	public ArenaTopRankingRewardsTemplate(Integer  id,String desc, int TopRanking, short honorPoint, short diamonds, int golds,
			String itemRewards, String itemNum) {
		super.setId(id);
		Desc = desc;
		this.TopRanking = TopRanking;
		HonorPoint = honorPoint;
		Diamonds = diamonds;
		Golds = golds;
		ItemRewards = itemRewards;
		ItemNum = itemNum;
	}
	
	public String getDesc() {
		return Desc;
	}

	public int getTopRanking() {
		return TopRanking;
	}

	public short getHonorPoint() {
		return HonorPoint;
	}
	public short getDiamonds() {
		return Diamonds;
	}
	public int getGolds() {
		return Golds;
	}
	public String getItemRewards() {
		return ItemRewards;
	}
	public String getItemNum() {
		return ItemNum;
	}
	
	
}
