package com.common.template;

import java.util.Map;

public class GachaTemplate {
	private Integer id ;
	private Short currencyType ;
	private Integer needCurrency ;
	private Short gachaTimes ;
	private Map<Integer, Integer> jackpot;
	private Integer freeGift ;
	public GachaTemplate(Integer id, Short currencyType, Integer needCurrency, Short gachaTimes,
			Map<Integer, Integer> jackpot, Integer freeGift) {
		super();
		this.id = id;
		this.currencyType = currencyType;
		this.needCurrency = needCurrency;
		this.gachaTimes = gachaTimes;
		this.jackpot = jackpot;
		this.freeGift = freeGift;
	}
	public Integer getId() {
		return id;
	}
	public Short getCurrencyType() {
		return currencyType;
	}
	public Integer getNeedCurrency() {
		return needCurrency;
	}
	public Short getGachaTimes() {
		return gachaTimes;
	}
	public Map<Integer, Integer> getJackpot() {
		return jackpot;
	}
	public Integer getFreeGift() {
		return freeGift;
	}
	
}
