package com.common.template;

public class UnionRewardTemplate {

	private int id;
	private int build;
	private String items;
	private String num;

	public UnionRewardTemplate(int id, int build, String items, String num) {
		super();
		this.id = id;
		this.build = build;
		this.items = items;
		this.num = num;
	}

	public int getId() {
		return id;
	}

	public int getBuild() {
		return build;
	}

	public String getItems() {
		return items;
	}

	public String getNum() {
		return num;
	}

	

}
