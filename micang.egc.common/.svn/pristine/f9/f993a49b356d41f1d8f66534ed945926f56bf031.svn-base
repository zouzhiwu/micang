package com.common.template;

public class ArenaPointsRewardsTemplate  extends TemplateBase{
	private short NeedArenaPoints;	
	private short HonorPoint; 
	private short	Diamonds; 
	private int	Golds; 
	private String	ItemRewards; 
	private String	ItemNum;
	
	public ArenaPointsRewardsTemplate(Integer  id, short needArenaPoints, short honorPoint, short diamonds, int golds,
			String itemRewards, String itemNum) {
		super.setId(id);
		NeedArenaPoints = needArenaPoints;
		HonorPoint = honorPoint;
		Diamonds = diamonds;
		Golds = golds;
		ItemRewards = itemRewards;
		ItemNum = itemNum;
	}
	public short getNeedArenaPoints() {
		return NeedArenaPoints;
	}
	public short getHonorPoint() {
		return HonorPoint;
	}
	public short getDiamonds() {
		return Diamonds;
	}
	public int getGolds() {
		return Golds;
	}
	public String getItemRewards() {
		return ItemRewards;
	}
	public String getItemNum() {
		return ItemNum;
	}
	
	
}
