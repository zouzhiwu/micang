package com.common.template;

import java.util.Map;

public class DropGroupTemplate {
	private Integer id;
	private Map<Integer, Double> groupProbability;
	private Map<Integer, Double> itemGroup;
	private Map<Integer, Double> memberGroup;
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Map<Integer, Double> getGroupProbability() {
		return groupProbability;
	}
	public void setGroupProbability(Map<Integer, Double> groupProbability) {
		this.groupProbability = groupProbability;
	}
	public Map<Integer, Double> getItemGroup() {
		return itemGroup;
	}
	public void setItemGroup(Map<Integer, Double> itemGroup) {
		this.itemGroup = itemGroup;
	}
	public Map<Integer, Double> getMemberGroup() {
		return memberGroup;
	}
	public void setMemberGroup(Map<Integer, Double> memberGroup) {
		this.memberGroup = memberGroup;
	}
	public DropGroupTemplate(Integer id, Map<Integer, Double> groupProbability, Map<Integer, Double> itemGroup,
			Map<Integer, Double> memberGroup) {
		super();
		this.id = id;
		this.groupProbability = groupProbability;
		this.itemGroup = itemGroup;
		this.memberGroup = memberGroup;
	}
	
}
