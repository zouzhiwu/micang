package com.common.template;

public class CharactervalueTemplate extends TemplateBase {
	
	private Byte rarity;
	private Integer weight;
	private Integer condition;
	private Integer attribute;
	private String attributeQuantity;
	private String txt;

	public CharactervalueTemplate(Integer id, Byte rarity, Integer weight, Integer condition, Integer attribute,
			String attributeQuantity, String txt) {
		super.setId(id);
		super.setName("name");
		
		this.rarity = rarity;
		this.weight = weight;
		this.condition = condition;
		this.attribute = attribute;
		this.attributeQuantity = attributeQuantity;
		this.txt = txt;
	}
	public Byte getRarity() {
		return rarity;
	}
	public void setRarity(Byte rarity) {
		this.rarity = rarity;
	}
	public Integer getWeight() {
		return weight;
	}
	public void setWeight(Integer weight) {
		this.weight = weight;
	}
	public Integer getCondition() {
		return condition;
	}
	public void setCondition(Integer condition) {
		this.condition = condition;
	}
	public Integer getAttribute() {
		return attribute;
	}
	public void setAttribute(Integer attribute) {
		this.attribute = attribute;
	}
	public String getAttributeQuantity() {
		return attributeQuantity;
	}
	public void setAttributeQuantity(String attributeQuantity) {
		this.attributeQuantity = attributeQuantity;
	}
	public String getTxt() {
		return txt;
	}
	public void setTxt(String txt) {
		this.txt = txt;
	}
}
